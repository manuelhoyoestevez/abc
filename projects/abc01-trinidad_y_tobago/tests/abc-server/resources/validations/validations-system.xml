<?xml version="1.0" encoding="UTF-8"?>


<validations name="System">


	<validation name="validateStateCode">

		<field name="stateCode">
			<field-validator type="required" />
			<!--
			<field-validator type="states_codes" />
			-->
		</field>

	</validation>


	<validation name="validateCreateIncident">

		<field name="incident">
			<field-validator type="required" />
		</field>

	</validation>


	<validation name="validateSendKioskCommand">

		<field name="kioskId">
			<field-validator type="required" />
		</field>

		<field name="command">
			<field-validator type="required" />
		</field>

	</validation>


	<validation name="validateSendGateCommand">

		<field name="gateId">
			<field-validator type="required" />
		</field>

		<field name="command">
			<field-validator type="required" />
		</field>

	</validation>


	<validation name="validateUpdateStepConfiguration">

		<field name="stepType">
			<field-validator type="required" />
		</field>

		<field name="active">
			<field-validator type="boolean" />
		</field>

		<field name="blocking">
			<field-validator type="boolean" />
		</field>

		<field name="orderNumber">
			<field-validator type="unsigned" />
		</field>

	</validation>


</validations>

